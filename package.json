    "url": "https://github.com/magic-FE/translate-man/issues"
  },
  "dependencies": {
    "vue": "^2.5.13",
    "vue-color": "^2.6.0",
    "vue-router": "^3.0.1",
    "vue-switches": "^2.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "archiver": "^2.1.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
