    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-vue": "^4.3.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^5.5.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
